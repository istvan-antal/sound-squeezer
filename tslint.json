{
    "rules": {
        "no-require-imports": false,
        "no-var-requires": false,
        
        "cyclomatic-complexity": [true, 7],
        "max-classes-per-file": [true, 5],
        "class-name": true,
        "comment-format": [
            true,
            "check-space"
        ],
        "indent": [true, "spaces", 4],
        "align": [true, "parameters", "statements"],
        "one-line": [
            true,
            "check-catch",
            "check-finally",
            "check-else",
            "check-open-brace",
            "check-whitespace"
        ],
        "no-var-keyword": true,
        "quotemark": [
            true,
            "single",
            "jsx-double",
            "avoid-escape"
        ],
        "semicolon": [
            true,
            "always",
            "ignore-bound-class-methods"
        ],
        "curly": true,
        "max-file-line-count": [true, 200],
        "max-line-length": [true, 120],
        "prefer-switch": [true, {"min-cases": 2}],
        "no-consecutive-blank-lines": [true, 1],
        "member-ordering": [true, {"order": "fields-first"}],
        "typedef": false,
        "typedef-whitespace": [
            true,
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            },
            {
                "call-signature": "onespace",
                "index-signature": "onespace",
                "parameter": "onespace",
                "property-declaration": "onespace",
                "variable-declaration": "onespace"
            }
        ],
        "promise-function-async": true,
        "member-access": [true, "no-public"],
        "no-irregular-whitespace": true,
        "import-spacing": true,
        "ordered-imports": false,
        "completed-docs": false,
        "interface-name": [true, "never-prefix"],
        "interface-over-type-literal": true,
        "match-default-export-name": false,
        "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
        "variable-name": [true, "allow-leading-underscore", "allow-pascal-case", "ban-keywords", "check-format"],
        "whitespace": [true, "check-branch", "check-operator", "check-typecast"],
        "arrow-parens": [true, "ban-single-arg-parens"],
        "arrow-return-shorthand": [true, "multiline"],
        "number-literal-format": true,
        "no-non-null-assertion": false,
        "no-reference": true,
        "no-internal-module": true,
        "no-namespace": true,
        "no-trailing-whitespace": true,
        "no-null-keyword": true,
        "no-unused-variable": true,
        "no-any": true,
        "jsdoc-format": true,
        "type-literal-delimiter": true,
        "switch-final-break": [true, "always"],
        "space-before-function-paren": [true, {"anonymous": "always", "named": "never", "method": "never", "asyncArrow": "always"}],
        "array-type": [true, "array-simple"],
        "object-literal-key-quotes": [true, "consistent-as-needed"],
        "object-literal-shorthand": true,
        "binary-expression-operand-order": false,
        "encoding": true,
        "newline-before-return": false,
        "file-header": false,
        "new-parens": true,
        "return-undefined": false,
        "no-angle-bracket-type-assertion": true,
        "no-boolean-literal-compare": false,
        "no-parameter-properties": true,
        "no-unnecessary-callback-wrapper": true,
        "no-unnecessary-initializer": true,
        "no-unnecessary-qualifier": true,
        "no-default-export": false,
        "no-mergeable-namespace": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-construct": true,
        "no-debugger": true,
        "no-void-expression": true,
        "no-duplicate-super": true,
        "no-duplicate-variable": [true, "check-parameters"],
        "no-empty": true,
        "no-eval": true,
        "no-floating-promises": true,
        "no-for-in-array": true,
        "no-inferred-empty-object-type": false,
        "no-invalid-template-strings": true,
        "no-invalid-this": [true, "check-function-in-method"],
        "no-misused-new": true,
        "no-object-literal-type-assertion": true,
        "no-unsafe-any": false,
        "no-unsafe-finally": true,
        "no-unused-expression": true,
        "no-use-before-declare": true,
        "no-unbound-method": [true, "ignore-static"],
        "no-shadowed-variable": true,
        "no-sparse-arrays": true,
        "no-string-literal": true,
        "no-switch-case-fall-through": true,
        "no-magic-numbers": true,
        "one-variable-per-declaration": true,
        "use-default-type-parameter": true,
        "unified-signatures": true,
        "no-inferrable-types": true,
        "use-isnan": true,
        "strict-boolean-expressions": false,
        "strict-type-predicates": true,
        "triple-equals": true,
        "switch-default": true,
        "radix": true,
        "restrict-plus-operands": true,
        "adjacent-overload-signatures": true,
        "prefer-for-of": true,
        "prefer-object-spread": true,
        "prefer-conditional-expression": true,
        "prefer-function-over-method": [true, "allow-public", "allow-protected"],
        "prefer-method-signature": false,
        "prefer-template": [true, "allow-single-concat"],
        "prefer-const": [true, {"destructuring": "all"}],
        "only-arrow-functions": true,
        "object-literal-sort-keys": false,
        "eofline": false,
        "linebreak-style": [true, "LF"],
        "await-promise": [true, "Thenable"],
        "ban": [
            true,
            "eval"
        ],
        "ban-types": [true, ["Object", "Use {} instead."], ["String"]],
        "no-import-side-effect": [false, {"ignore-module": "(\\.html|\\.css)$"}],
        "import-blacklist": [true, "rxjs", "lodash"],
        "forin": true,
        "label-position": true,
        "no-arg": true
    }
}